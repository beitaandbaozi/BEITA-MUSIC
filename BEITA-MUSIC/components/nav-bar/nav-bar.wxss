.nav {
  display: flex;
  height: 88rpx;
  color: #fff
}

.nav .left,
.nav .right,
.nav .center {
  display: flex;
  justify-content: center;
  align-items: center;
}

.nav .left,
.nav .right {
  width: 120rpx;
}

.nav .center {
  flex: 1
}

.default {
  display: none;
}

.slot:empty+.default {
  display: block;
}